{
    "name": "laravel-lang/locales",
    "description": "Basic functionality for working with localizations",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "locale",
        "locales",
        "localization",
        "translation",
        "translations"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro"
        },
        {
            "name": "Laravel-Lang Team",
            "homepage": "https://laravel-lang.com"
        }
    ],
    "support": {
        "issues": "https://github.com/Laravel-Lang/locales/issues",
        "source": "https://github.com/Laravel-Lang/locales"
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "archtechx/enums": "^0.3",
        "dragon-code/support": "^6.11",
        "illuminate/collections": "*",
        "illuminate/support": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.25 || ^7.22 || ^8.0 || ^9.0",
        "pestphp/pest": "^2.24",
        "symfony/var-dumper": "^5.0 || ^6.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "LaravelLang\\Locales\\": "src/"
        },
        "files": [
            "helpers/lang.php",
            "helpers/collection.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dragon-code/codestyler": true,
            "ergebnis/composer-normalize": true,
            "friendsofphp/php-cs-fixer": true,
            "symfony/thanks": true,
            "pestphp/pest-plugin": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLang\\Locales\\ServiceProvider"
            ]
        }
    }
}
